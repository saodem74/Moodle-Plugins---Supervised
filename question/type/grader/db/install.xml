<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/grader/db" VERSION="20110405" COMMENT="XMLDB file for Moodle question/type/grader"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="question_grader" COMMENT="Abstract autograder">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="studentshowoptionsgrp"/>
        <FIELD NAME="studentshowoptionsgrp" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Defines what to show to student after testing his answer" PREVIOUS="id" NEXT="teachershowoptionsgrp"/>
        <FIELD NAME="teachershowoptionsgrp" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Defines what to show in report to teacher" PREVIOUS="studentshowoptionsgrp"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>