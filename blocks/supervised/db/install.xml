<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/supervised/db" VERSION="20110115" COMMENT="XMLDB file for Moodle blocks/supervised"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="block_supervised" COMMENT="Supervised block" NEXT="block_supervised_classroom">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="logid"/>
        <FIELD NAME="logid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="id" NEXT="classroomid"/>
        <FIELD NAME="classroomid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="logid" NEXT="courseid"/>
        <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="classroomid" NEXT="groupid"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="courseid" NEXT="starttimework"/>
        <FIELD NAME="starttimework" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="groupid" NEXT="timework"/>
        <FIELD NAME="timework" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="starttimework" NEXT="lecturerid"/>
        <FIELD NAME="lecturerid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="timework" NEXT="typeaction"/>
        <FIELD NAME="typeaction" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false" PREVIOUS="lecturerid" NEXT="twinkeyid"/>
        <FIELD NAME="twinkeyid" TYPE="int" LENGTH="20" NOTNULL="true" UNSIGNED="false" SEQUENCE="false" PREVIOUS="typeaction"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="block_supervised_classroom" COMMENT="Stores data about classroom" PREVIOUS="block_supervised">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="number"/>
        <FIELD NAME="number" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" PREVIOUS="id" NEXT="initialvalueip"/>
        <FIELD NAME="initialvalueip" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" COMMENT="The initial value of the range IP address" PREVIOUS="number" NEXT="finishvalueip"/>
        <FIELD NAME="finishvalueip" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" COMMENT="The finish value of the range IP address" PREVIOUS="initialvalueip"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>